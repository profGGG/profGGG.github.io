	keywords = {Computer Science - Artificial Intelligence, Computer Science - Machine Learning, Statistics - Machine Learning},
	file = {arXiv Fulltext PDF:C\:\\Users\\gwhap\\Zotero\\storage\\9PD66BQB\\Plecko and Bareinboim - 2024 - Fairness-Accuracy Trade-Offs A Causal Perspective.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\gwhap\\Zotero\\storage\\UCLWKD38\\2405.html:text/html},
}

