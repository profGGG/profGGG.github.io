Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {microfluidics, polymerase chain reaction, extreme PCR, fast PCR, heat transfer, PCR reactor design, thermal cycling},
	pages = {1533},
	file = {Full Text PDF:C\:\\Users\\gwhap\\Zotero\\storage\\EJYABITE\\Madadelahi and Madou - 2023 - Rational PCR Reactor Design in Microfluidics.pdf:application/pdf},
}

