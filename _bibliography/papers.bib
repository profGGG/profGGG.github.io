	journal = {PLOS Neglected Tropical Diseases},
	author = {Müller, Katharina and Daßen, Sarah and Holowachuk, Scott and Zwirglmaier, Katrin and Stehr, Joachim and Buersgens, Federico and Ullerich, Lars and Stoecker, Kilian},
	editor = {Demeure, Christian E.},
